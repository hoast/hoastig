{
  "author": {
    "name": "Ron Dekker",
    "url": "https://rondekker.nl"
  },
  "name": "hoastig",
  "version": "2.0.0-alpha.1",
  "description": "Static page generator using hoast.",
  "license": "ISC",
  "keywords": [
    "hoast",
    "hoastig",
    "static",
    "page",
    "generator"
  ],
  "homepage": "https://github.com/hoast/hoastig#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hoast/hoastig.git"
  },
  "bugs": {
    "url": "https://github.com/hoast/hoastig/issues"
  },
  "engines": {
    "node": ">=7.8.0"
  },
  "preferGlobal": true,
  "main": "library/index.js",
  "bin": {
    "hoastig": "bin/hoastig.js"
  },
  "files": [
    "bin/",
    "library/"
  ],
  "scripts": {
    "coverage": "nyc report --reporter=text-lcov > coverage.lcov && codecov",
    "posttest": "eslint library/**/*.js bin/**/*.js test/*/*.js",
    "test": "nyc --reporter=text ava \"test/*/*.js\" --verbose"
  },
  "dependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-typescript": "^7.1.0",
    "commander": "^2.19.0",
    "gray-matter": "^4.0.2",
    "highlightjs": "^9.12.0",
    "hoast": "^1.1.6",
    "hoast-changed": "^1.1.2",
    "hoast-convert": "^2.0.0",
    "hoast-filter": "^1.1.1",
    "hoast-frontmatter": "^1.2.1",
    "hoast-layout": "^1.3.0",
    "hoast-minify": "^1.2.0",
    "hoast-transform": "^1.1.1",
    "jstransformer-handlebars": "^1.1.0",
    "jstransformer-markdown-it": "^2.1.0",
    "markdown-it-anchor": "^5.0.2",
    "postcss": "^7.0.11",
    "postcss-preset-env": "^6.5.0",
    "semver": "^5.6.0"
  },
  "devDependencies": {
    "ava": "^1.1.0",
    "codecov": "^3.1.0",
    "debug": "^4.1.1",
    "eslint": "^5.12.0",
    "nyc": "^13.1.0"
  }
}
